{
  "name": "kronos-endpoint",
  "version": "0.0.0-semantic-release",
  "description": "named communication (end)-points inside of kronos",
  "main": "dist/Endpoint.js",
  "module": "src/Endpoint.js",
  "keywords": [],
  "scripts": {
    "cover": "nyc npm test",
    "test": "mocha tests/*_test.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "docs": "jsdoc2md -l off -t doc/README.hbs -f src/*.js >README.md",
    "prepublish": "rollup --output=dist/Endpoint.js -c -- src/Endpoint.js",
    "pretest": "npm run prepublish",
    "posttest": "markdown-doctest",
    "precover": "npm run prepublish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/kronos-endpoint.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/kronos-endpoint/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/kronos-endpoint#readme",
  "dependencies": {
    "kronos-interceptor": "^2.4.16"
  },
  "devDependencies": {
    "kronos-test-interceptor": "^2.1.0",
    "chai": "^4.0.2",
    "mocha": "^3.4.2",
    "semantic-release": "^6.3.6",
    "jsdoc-to-markdown": "^3.0.0",
    "markdown-doctest": "^0.9.1",
    "rollup": "^0.45.1",
    "nyc": "^11.0.3"
  },
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "engines": {
    "node": ">=8.1.3"
  },
  "nyc": {
    "include": [
      "dist/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./coverage"
  },
  "template": {
    "repository": {
      "url": "https://github.com/Kronos-Tools/npm-package-template.git"
    }
  }
}