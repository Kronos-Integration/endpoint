{
  "name": "@kronos-integration/endpoint",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./src/module.mjs",
  "exports": {
    ".": "./src/module.mjs",
    "./": "./"
  },
  "description": "named communication (end)-points inside of kronos",
  "keywords": [
    "endpoint"
  ],
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "test": "ava --timeout 2m",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme ./src/module.mjs --section=API",
    "lint": "documentation lint ./src/module.mjs"
  },
  "dependencies": {
    "@kronos-integration/interceptor": "^9.1.0"
  },
  "devDependencies": {
    "ava": "^3.8.1",
    "c8": "^7.1.2",
    "documentation": "^12.3.0",
    "semantic-release": "^17.0.7"
  },
  "engines": {
    "node": ">=14.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/endpoint.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/endpoint/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/endpoint#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-kronos-module",
      "arlac77/template-github"
    ]
  }
}
