{
  "name": "kronos-endpoint",
  "version": "0.0.0-semantic-release",
  "description": "named communication (end)-points inside of kronos",
  "main": "dist/endpoint.js",
  "module": "src/endpoint.js",
  "keywords": [
    "endpoint"
  ],
  "scripts": {
    "cover": "nyc --temp-directory build/nyc npm test",
    "test": "mocha tests/*-test.js",
    "semantic-release": "semantic-release",
    "docs": "jsdoc2md -l off -t doc/README.hbs -f src/*.js >README.md",
    "pretest": "rollup -c",
    "posttest": "markdown-doctest",
    "precover": "rollup -c",
    "prepare": "rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kronos-Integration/kronos-endpoint.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/kronos-endpoint/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/kronos-endpoint#readme",
  "dependencies": {
    "kronos-interceptor": "^2.5.1"
  },
  "devDependencies": {
    "kronos-test-interceptor": "^2.1.0",
    "chai": "^4.1.2",
    "mocha": "^4.0.1",
    "semantic-release": "^10.0.1",
    "jsdoc-to-markdown": "^3.0.2",
    "markdown-doctest": "^0.9.1",
    "rollup": "^0.52.0",
    "nyc": "^11.4.0"
  },
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "engines": {
    "node": ">=8.9.1"
  },
  "nyc": {
    "include": [
      "dist/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./coverage"
  },
  "template": {
    "repository": {
      "url": "https://github.com/Kronos-Tools/npm-package-template.git"
    }
  },
  "xo": {
    "space": true
  }
}
